.container {
    margin-top: 2rem;
}

.info {
    padding: 2rem .5rem 1rem .5rem;
    /* border-top: 3px solid var(--primary-color); */
    border-bottom: 3px solid var(--primary-color);
}

input[type="checkbox"].edit-checkbox {
    display:none;
}
input[type="checkbox"].edit-checkbox:checked ~ .edit,
input[type="checkbox"].edit-checkbox:checked ~ .edit-background {
	display: block;
}

.edit {
    position: fixed;
    float: left;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    margin: auto;
    display: none;
    background-color: #fff;
    z-index: 9999;
}

.edit > p {
	padding: 15px;
    margin: 0;
}

.edit-header {
	background-color: #f9f9f9;
    border-bottom: 1px solid #dddddd;
    box-sizing: border-box;
    height: 50px;
}

.edit-header h3 {
	margin: 0;
    box-sizing: border-box;
    padding-left: 15px;
    line-height: 50px;
    color: #4d4d4d;
    font-size: 16px;
    display: inline-block;
}

.edit-header label {
	box-sizing: border-box;
    border-left: 1px solid #dddddd;
    float: right;
    line-height: 50px;
    padding: 0 15px 0 15px;
    cursor: pointer;
}

.edit-header label:hover img {
	opacity: 0.6;
}

.options-td {
    /* background-color: var(--dark-secondary-color); */
}

.action-btn {
    cursor: pointer;
    color: white;
    background-color: rgb(170, 170, 170);
    padding: .5rem;
    border-radius: .25rem;
}

.view-btn {
    display: none;
}

#refresh {
    visibility: hidden;
}

.refresh-btn {
    cursor: pointer;
}

.btn {
    /* padding: .25rem; */
    border-radius: .25rem;
    /* width: 5rem; */
}

.btn > * {
    text-align: center;
}

.btn-delete {
    background: red;
    cursor: pointer;
}

.btn-add {
    background: green;
    cursor: pointer;
}

#edit-popup {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 400;
}

#edit-popup-wrapper {
    width: 100vw;
    height: 100vh;
    background-color: rgba(0, 0, 0, 0.5);
    display: flex;
    align-items: center;
    justify-content: center;
}

#edit-img, #edit-img2 {
    /* width: 20rem; */
    object-fit: cover;
    object-position: right;
}

#edit-popup-box {
    height: auto;
    background-color: var(--light-primary-color);
    max-height: 96dvh;
    padding: 1rem;
    overflow-y: auto;
    box-shadow: 0px 0px 50px 0px black;
}

#top-bar {
    width: 100%;
    height: 1.5rem;
    display: flex;
    justify-content: flex-end;
}

#close-btn {
    font-size: 1.5rem;
}

#close-btn:hover {
    cursor: pointer;
}

#form, #form {
    display: flex;
    flex-direction: column;
    /* height: calc(100% - 1.5rem); */
}

/* #form > *, #form > * {
    display: flex;
    flex-direction: column;
} */

.edit-flex-row {
    display: flex;
    flex-direction: row !important;
    justify-items: center;
    /* align-items: center; */
    width: 100%;
    gap: 1rem;
}
.edit-flex-row > *{
    width: 100%;
}

#form input, #form select, #form textarea {
    font-size: 1.25rem;
}

#form input:not([type="file"]):not([type="radio"]) {
    width: 100%;
    margin: .5rem 0 .5rem 0;
    padding: .25rem;
    color: white;
    background-color: transparent;
    border: 1px solid #ccc;
    /* height: 2rem; */
}

#form input[type="radio"] {
    margin: .5rem;
    padding: .25rem;
    color: white;
    background-color: transparent;
    border: 1px solid #ccc;
    height: 1.25rem;
}

.modo-fieldset {
    display: flex;
    flex-direction: column;
}

.modo-fieldset-item {
    width: 100%;
    /* cursor: pointer; */
}

.modo-fieldset-item > input[type="radio"] {
    /* height: 2rem; */
    cursor: pointer;
    scale: 1.25;
}

.modo-fieldset-item > label {
    /* cursor: pointer; */
}


#form select {
    width: 100%;
    margin: .5rem 0 .5rem 0;
    padding: .25rem;
    color: white;
    background-color: transparent;
    border: 1px solid #ccc;
    height: 2rem;
}

#form textarea {
    width: 100%;
    margin: .5rem 0 .5rem 0;
    padding: .25rem;
    color: white;
    background-color: transparent;
    border: 1px solid #ccc;
    resize: vertical;
}

#form textarea.noresize {
    resize: none;
}


#form select option {
    color: var(--bg-color);
    background-color: white;
}

#info-title {
    text-align: center;
    font-weight: bolder;
    font-size: xx-large;
    padding-bottom: 1rem;
}

.refresh-btn {
    text-align: right;
    margin: 1rem;
}

.edit-flash-msg {
    text-align: center;
    color: orange;
    padding: 1rem;
}

#cpf_email-input-wrapper {
    display: flex;
    align-items: center;
}

#cpf_email {
    width: 70%;
    border-right: 0 !important;
}

#cpf_email-btn {
    width: 40%;
    height: 2rem;
    background: #321555;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    /* border-width: 1px 1px 1px 0;
    border-style: solid;
    border-color: #ccc; */
}

#cpf_email-btn:hover {
    cursor: pointer;
}

#cpf_email-btn-text {
    text-align: center;
    color: white;
}

#info-bottom {
    margin-top: 3rem;
    margin-bottom: 1rem;
    visibility: hidden;
}

.info-bottom-btns {
    display: flex;
    width: 100%;
    gap: 1rem;
}
 
.info-main-btn {
    background: linear-gradient(to right, #321555 0%,#2a1149 100%);
}

.info-btn {
    border: 1px solid white;
    width: 100%;
    text-align: center;
    /* height: 2.5rem; */
    font-weight: bolder;
    font-size: x-large;
    margin: 1rem 0 1rem 0;
    padding: 1rem 0 1rem 0;
    background-size: 400% 400%;
    animation: info-btn-bg-gradient 4s ease-in-out alternate infinite;
    color: white;
    scale: 1;
}
@keyframes info-btn-bg-gradient {
    0% {
        background-position: 0% 50%;
    }
    50% {
        background-position: 100% 50%;
    }
    100% {
        background-position: 0% 50%;
    }
}

.info-btn:hover {
    border: none;
}

.info-btn:not(.edit-delete-btn):hover {
    cursor: pointer;
    animation: info-btn-hover .4s ease-out forwards;
    transition-delay: 0.1s;
}

@keyframes info-btn-hover {
    0% {
    }

    50%  {
        scale: 0.90;
        background: transparent;
        outline: 1px solid white;
        box-shadow: 0 0 5px 1px #2a1149;
    }

    100% {
        scale: 1;
        background: transparent;
        outline: 1px solid white;
        box-shadow: 0 0 5px 2px #2a1149;
    }
}

.edit-delete-btn:hover {
    cursor: pointer;
    animation: edit-delete-btn-hover .4s ease-out forwards;
    transition-delay: 0.1s;
}

@keyframes edit-delete-btn-hover {
    0% {
    }

    50%  {
        scale: 0.90;
        background: transparent;
        outline: 1px solid white;
        box-shadow: 0 0 5px 1px red;
    }

    100% {
        scale: 1;
        background: transparent;
        outline: 1px solid white;
        box-shadow: 0 0 5px 2px red;
    }
}


.edit-bottom-msg {
    color: #e0c5ff;
    text-align: right;
    padding-right: 0.1rem;
}

.info-btn, #signin-btn {
    color: white;
    font-weight: 600;
    font-size: large;
}

.info-btn:hover, #signin-btn:hover {
    cursor: pointer;
    color: #e0c5ff;
    text-shadow: 0 0 1px white;
}

.edit-subtitle {
    padding: 1rem 0 1rem 0;
    font-weight: bold;
    font-size: large;
    text-align: center;
}

#cad_end_cep {
    /* max-width: 8rem; */
}

#cad_end_num {
    /* min-width: 2.5rem;
    max-width: 5rem; */
}

#cad_end_uf {
    text-transform: uppercase;
    /* min-width: 2.5rem;
    max-width: 3rem; */
}

/* image upload */


.file-upload {
    /* background-color: #ffffff; */
    width: 100%;
    /* margin: 0 auto; */
    /* padding: 20px; */
    border: 1px solid white;
    margin: 1rem 0 1rem 0;
}

.file-upload-btn {
    width: 100%;
    margin: 0;
    color: #fff;
    background: #1FB264;
    border: none;
    padding: 10px;
    border-radius: 4px;
    border-bottom: 4px solid #15824B;
    transition: all .2s ease;
    outline: none;
    text-transform: uppercase;
    font-weight: 700;
}

.file-upload-btn:hover {
    background: #1AA059;
    color: #ffffff;
    transition: all .2s ease;
    cursor: pointer;
}

.file-upload-btn:active {
    border: 0;
    transition: all .2s ease;
}

.file-upload-content:not(.show-img) {
    display: none;
    text-align: center;
}

.show-img {
    display: box;
    text-align: center;
}

.file-upload-input {
    position: absolute;
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    outline: none;
    opacity: 0;
    cursor: pointer;
}

.image-upload-wrap {
    /* margin: 1rem 0 1rem 0;
    border: 1px solid white; */
    /* border: 4px dashed #1FB264; */
    position: relative;
}

.image-upload-wrap:hover {
    background: var(--primary-color);
}
.file-dragover {
    background: var(--primary-color);
}

.image-title-wrap {
    padding: 0 15px 15px 15px;
    color: #222;
}

.drag-text {
    display: flex;
    justify-content: center;
    align-items: center;
    /* text-align: center; */
    height: 4rem;
    height: 100%;
    gap: 1rem;
}

.drag-text h3 {
    font-weight: 100;
    text-transform: uppercase;
    color: white;
    padding: 60px 0;
}

.file-upload-image {
    width: 100%;
    object-fit: contain;
    max-height: 500px;
    padding: .5rem;
    /* max-height: 200px;
    max-width: 200px; */
    /* margin: auto; */
    /* padding: 20px; */
}

.remove-image {
    width: 200px;
    margin: 0;
    color: #fff;
    background: #cd4535;
    border: none;
    padding: 10px;
    border-radius: 4px;
    border-bottom: 4px solid #b02818;
    transition: all .2s ease;
    outline: none;
    text-transform: uppercase;
    font-weight: 700;
}

.remove-image:hover {
    background: #c13b2a;
    color: #ffffff;
    transition: all .2s ease;
    cursor: pointer;
}

.remove-image:active {
    border: 0;
    transition: all .2s ease;
}

.address-wrapper {
    background: var(--bg-color);
    padding: .5rem;
}

.address-rows {
    display: flex;
    flex-direction: column;
    gap: .5rem;
}

.address-row {
    width: 100%;
    height: 1rem;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    gap: .25rem;
    padding: 0;
    margin: .75rem 0 .75rem 0;
}

.address-add {
    font-size: 1rem;
    padding: .25rem;
    margin: .25rem 0 .25rem 0;
    cursor: pointer;
    color: green;
    /* background: white; */
    background: white;
    border: 1px solid green;
    text-align: center;
}

.address-input {
    width: 20% !important;
}

.address-btn {
    font-size: 1rem;
    padding: .35rem;
    cursor: pointer;
    color: #ccc;
    /* background: white; */
    background: transparent;
    border: 1px solid #ccc;
    text-align: center;
    margin: .25rem;
}

.stage-details-item {
    display: flex;
    align-items: center;
    margin: .25rem 0 .25rem .5rem;
}

#stage-details-tax {
    display: none;
}

.satage-details-item-img {
    width: 3rem;
    margin: 0 .5rem 0 0;
}

.light-text {
    color: #ccc;
    font-size: .85rem;
}

.action-group {
    margin-left: auto;
    display: flex;
}

@media (max-width: 1280px) {
    #edit-img, #edit-img2 {
        width: 20rem;
    }
}

@media (max-width: 960px) {
    #edit-img, #edit-img2 {
        width: 10rem;
    }   
}

@media (max-width: 840px) {
    #edit-img, #edit-img2 {
        width: 0;
    }   
}

@media (max-width: 720px) {
    #edit-popup-box {
        min-width: 15rem;
    }
    #cpf_email-btn-text span {
        display: none;
    }
    #edit-popup-box {
        max-width: 90vw;
    }
}



@media (min-width: 0) {
    #form input, #form select, #form textarea {
        font-size: 1rem;
    }
    #edit-popup-box {
        min-width: unset;
        max-width: unset;
        width: 95%;
    }
}
@media (min-width: 768px) {
    #form input, #form select, #form textarea {
        font-size: 1.15rem;
    }
    #edit-popup-box {
        min-width: unset;
        max-width: unset;
        width: 80%;
    }    
}
@media (min-width: 992px) {
    #form input, #form select, #form textarea {
        font-size: 1.25rem;
    }
    #edit-popup-box {
        min-width: 30rem;
        max-width: 80vw;
        width: 70%;
    }    
}
@media (min-width: 1200px) {
    #form input, #form select, #form textarea {
        font-size: 1.5rem;
    }
    #edit-popup-box {
        min-width: 30rem;
        max-width: 80vw;
        width: unset;
    }  
}
